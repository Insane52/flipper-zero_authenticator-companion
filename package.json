{
  "name": "flipper-authenticator-companion",
  "main": "main.js",
  "version": "0.0.1",
  "build": {
    "icon": "public/favicon.png",
    "productName": "Flipper Authenticator Companion",
    "files": [
      "public/**/*",
      "main.js"
    ],
    "win": {},
    "linux": {},
    "mac": {}
  },
  "scripts": {
    "electron": "wait-on http://localhost:8080 && electron .",
    "electron-dev": "concurrently \"yarn run dev\" \"yarn run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -s"
  },
  "devDependencies": {
    "@fontsource/material-icons": "^5.0.7",
    "@fontsource/roboto": "^5.0.8",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@smui/button": "^7.0.0-beta.14",
    "@smui/list": "^7.0.0-beta.14",
    "concurrently": "^8.2.0",
    "delay": "^6.0.0",
    "electron": "^25.5.0",
    "electron-builder": "^24.6.3",
    "escape-string-regexp": "^5.0.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.65.1",
    "serialport": "^11.0.1",
    "sirv-cli": "^2.0.2",
    "smart-buffer": "^4.2.0",
    "svelte": "^4.1.2",
    "svelte-material-ui": "^7.0.0-beta.14",
    "svelte-preprocess": "^5.0.4",
    "svelte-routing": "^2.0.0",
    "svelte-waypoint": "^0.1.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "electron-serve": "^1.1.0"
  }
}
